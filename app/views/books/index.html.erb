<%= render 'layouts/mypagebook', {user: @user, book: @book} %>
<table>
  <thead>
    <tr>
      <th>title</th>
      <th>opinion</th>
    </tr>
  </thead>
  <tbody>
  <% @books.each do |book| %>
    <tr>
      <td><%= book.title %></td>
      <td><%= book.body %></td>
    </tr>
  <% end %>
  </tbody>
</table>
<% if flash[:notice] %>
  <p id=notice><%= flash[:notice] %></p>
<% end %>